{
    "extends": "airbnb-base",
    "rules": {
        "max-len": ["error", { "code": 200 }],
        "class-methods-use-this": 0,
        "no-console": "error"
    },
    "globals": {
        "window": true,
        "document": true
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    }
}